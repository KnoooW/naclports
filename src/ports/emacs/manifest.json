{
  "name": "Emacs",
  "description": "Emacs console application running using NativeClient",
  "version": "24.3.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "file_handlers": {
    "edit": {
      "types": [
        "text/*"
      ],
      "extensions": [
        "json",
        "txt",
        "js",
        "html",
        "h",
        "c",
        "cc",
        "cpp",
        "java"
      ],
      "title": "Edit with Emacs"
    }
  },
  "permissions": [
    "fileSystem",
    "storage",
    "unlimitedStorage"
  ]
}
